          <div class="rlr-product-card__detail-wrapper rlr-js-detail-wrapper">
            <!-- Product card header -->
            <header class="rlr-product-card__header">
              <div>
                <a href="/tours/details/{{this.url}}" class="rlr-product-card__anchor-title">
                  <h2 class="rlr-product-card__title" itemprop="name">{{this._doc.title}}</h2>
                </a>
                <div>
                  <a href="/tours/details/{{this.url}}" class="rlr-product-card__anchor-cat">
                    <span class="rlr-product-card__sub-title">{{this._doc.type}}</span>
                  </a>
                </div>
              </div>
            </header>
            <!-- Product card footer -->
            <div class="rlr-product-card__footer">
              <div class="rlr-icon-text rlr-product-card__icon-text"><i
                  class="rlr-icon-font flaticon-three-o-clock-clock"> </i> <span class="">{{this._doc.duration}}
                  Day </span></div>
              <ul class="rlr-product-card__icon-text-list">
                <li class="rlr-icon-text rlr-product-card__icon-text"><i class="rlr-icon-font flaticon-check">
                  </i> <span class="rlr-icon-text__title">1-56 member group </span></li>
                <li class="rlr-icon-text rlr-product-card__icon-text"><i class="rlr-icon-font flaticon-check">
                  </i> <span class="rlr-icon-text__title">Free Cancellation* </span></li>
              </ul>
            </div>
          </div>
        </article>
        {{/each}}
      </div>
      <div style="display: flex;">
        <a href="/tours" style="margin:auto">
          <button
            class="btn rlr-button btn rlr-button custom-class rlr-button--medium rlr-button--rounded rlr-button__color--white rlr-button--brand rlr-button-- rlr-button-- rlr-button__color-- rlr-button-- rlr-button--">View
            all</button>
        </a>
      </div>
    </section>
    <!-- Product Carousel -->
    <section class="rlr-section rlr-section__mb pc_view" style="padding-top: 15px;">
      <div class="container">
        <!-- Swiper -->
        <div class="rlr-carousel__items">
          <div class="swiper rlr-js-product-card-swiper">
            <!-- Carousel header -->
