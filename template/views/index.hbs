      <div class="container ">
        <div class="rlr-section-header">
          <!-- Section heading -->
          <div class="rlr-section__title">
            <h2 class="rlr-section__title--main">Tour Package</h2>
            <span class="rlr-section__title--sub">Agra Mathura Vrindavan Tour Packages: Explore the Sacred Sites of
              India</span>
          </div>
        </div>
        {{#each tours}}
        <article class="rlr-product-card rlr-product-card--v3 thin_shadow" itemscope
          itemtype="https://schema.org/Product" style="margin: 15px 0px;">

          <figure class="rlr-product-card__image-wrapper">
            <a href="/tours/details/{{this.url}}">
              <div class="swiper rlr-js-product-multi-image-swiper">
                <div class="swiper-wrapper" style="height: 171px;">
                  {{#with this}}
                  {{#each this._doc.images}}
                  <div class="swiper-slide">
                    <img itemprop="image" data-sizes="auto" src="/assets/tours/{{../this._doc.id}}/{{this.url}}"
                      class="lazyload" alt="product-image" />
                  </div>
                  {{/each}}
                  {{/with}}
                </div>
                <button type="button" class="btn rlr-button splide__arrow splide__arrow--prev" aria-label="prev button">
                  <i class="rlr-icon-font flaticon-left-chevron"> </i>
                </button>
                <button type="button" class="btn rlr-button splide__arrow splide__arrow--next" aria-label="next button">
                  <i class="rlr-icon-font flaticon-chevron"> </i>
                </button>
              </div>
